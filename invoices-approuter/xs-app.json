{
    "welcomeFile": "/index.html",
    "authenticationMethod": "none",
    "logout":{
        "logoutEndPoint": "/do/logout"
    },
    "routes": [
        {
            "authenticationType": "none",
            "source": "^/V2/Northwind/Northwind.svc/(.*)$",
            "destination": "Northwind",            
            "csrfProtection": true,
            "target": "$1"            
        },
        {
            "authenticationType": "xsuaa",
            "source": "^(.*)$",
            "destination": "Northwind",
            "service": "html5-apps-repo-rt",
            "target": "$1"    
        }
    ]
}